<Window x:Class="tomcat_easy_copy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tomcat_easy_copy"
        mc:Ignorable="d"
        Title="Easy TomCat" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <Menu Margin="0,0,0,282">
            <MenuItem x:Name="MenuItemFile" Height="23" Width="66" Header="Copy" FontSize="16" Click="MenuItemFile_Click"/>
            <MenuItem x:Name="MenuItemView" Height="23" Width="86" FontSize="16" Header="Delete" Click="MenuItemView_Click"/>
        </Menu>
        <Image x:Name="image" HorizontalAlignment="Left" Height="248" Margin="101,61,0,0" VerticalAlignment="Top" Width="326" Source="images/tomcat.png"/>
    </Grid>
</Window>
